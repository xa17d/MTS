        <activity android:name="moco.android.mtsdevice.triage.TriageSalvageinfoAdd"></activity>
        
        <activity android:name="moco.android.mtsdevice.salvage.SalvageActivity"></activity>
        
        <activity android:name="moco.android.mtsdevice.therapy.TherapyListActivity"></activity>
        <activity android:name="moco.android.mtsdevice.therapy.TherapyAreaActivity"></activity>
        <activity android:name="moco.android.mtsdevice.therapy.TherapyVitalParameterActivity"></activity>
        
